<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SelectEnum">
        <attr name="enum_class" format="string" />
        <attr name="enum_value" format="string" />
        <attr name="enum_columns" format="integer" />
        <attr name="enum_display_values" format="reference" />
    </declare-styleable>

    <attr name="enumSpinnerStyle" format="reference" />

    <style name="EnumSpinner">
        <item name="android:textColor">@color/dy_dark</item>
        <item name="android:background">@color/dy_white</item>
        <item name="android:textSize">@dimen/txt_small</item>
        <item name="android:popupBackground">@color/dy_dark</item>
    </style>
</resources>